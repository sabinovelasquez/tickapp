    link(rel="stylesheet", type="text/css", href="{% static 'premises/style.css' %}")
  body
    h1 Hell
    | {% if latest_premise_list %}
    ul
      | {% for premise in latest_premise_list %}
      li
        a(href="/premises/{{ premise.name }}/")
